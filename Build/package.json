{
	"name": "bootstrap5-webpack",
	"version": "1.0.0",
	"description": "Bootstrap 5, Webpack, PostCSS, Stylelint, ESLint.",
	"type": "module",
	"scripts": {
		"build": "webpack --config webpack.config.production.js --mode production",
		"dev": "webpack --config webpack.config.development.js --mode development",
		"watch": "webpack --config webpack.config.development.js --mode development --watch",
		"eslint": "eslint .",
		"eslint.fix": "eslint --fix .",
		"eslint.junit": "eslint -f junit -o reports/eslintreport.junit.xml Assets/**/*.js",
		"stylelint.json": "stylelint --config stylelint.config.js -f json -o reports/stylelintreport.json Assets/Scss/**/*.scss",
		"stylelint.junit": "stylelint --config stylelint.config.js --custom-formatter node_modules/stylelint-checkstyle-reporter/dist/stylelint-checkstyle-reporter.mjs -o reports/stylelintreport.junit.xml Assets/Scss/**/*.scss",
		"stylelint.verbose": "stylelint --config stylelint.config.js -f verbose -o reports/stylelintreport.verbose.txt Assets/Scss/**/*.scss"
	},
	"author": "ITZBund",
	"license": "GPL-3.0-or-later",
	"dependencies": {
		"@popperjs/core": "^2.11.8",
		"bootstrap": "^5.3.3",
		"mediaelement": "5.1.1",
		"swiper": "^11.1.4"
	},
	"devDependencies": {
		"@eslint/js": "9.6.0",
		"autoprefixer": "10.4.19",
		"copy-webpack-plugin": "12.0.2",
		"css-loader": "7.1.2",
		"eslint": "9.6.0",
		"eslint-formatter-junit": "8.40.0",
		"eslint-webpack-plugin": "4.2.0",
		"fast-glob": "3.3.2",
		"globals": "15.8.0",
		"mini-css-extract-plugin": "2.9.0",
		"path": "0.12.7",
		"postcss": "8.4.39",
		"postcss-inline-svg": "6.0.0",
		"postcss-loader": "8.1.1",
		"postcss-preset-env": "9.6.0",
		"postcss-pxtorem": "6.1.0",
		"postcss-svgo": "7.0.1",
		"sass": "1.77.6",
		"sass-loader": "14.2.1",
		"style-loader": "4.0.0",
		"stylelint": "16.6.1",
		"stylelint-checkstyle-reporter": "1.0.0",
		"stylelint-config-recommended": "14.0.1",
		"stylelint-config-recommended-scss": "14.0.0",
		"stylelint-config-standard": "36.0.1",
		"stylelint-order": "6.0.4",
		"stylelint-scss": "6.3.2",
		"stylelint-webpack-plugin": "5.0.1",
		"webpack": "5.92.1",
		"webpack-cli": "5.1.4",
		"xml2js": "0.6.2"
	}
}
