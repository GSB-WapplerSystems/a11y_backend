{
	"name": "a11y-webpack",
	"version": "1.0.0",
	"description": "Webpack, PostCSS, Stylelint, ESLint.",
	"main": "index.js",
	"private": true,
	"scripts": {
		"build": "webpack --config webpack.config.production.js --mode production",
		"dev": "webpack --config webpack.config.development.js --mode development",
		"watch": "webpack --config webpack.config.development.js --mode development --watch",
		"lint:eslint": "eslint Assets/**/*.js",
		"lint:eslint.fix": "eslint --fix Assets/**/*.js",
		"lint:eslint.report.junit": "eslint -f junit -o reports/eslintreport.junit.xml Assets/**/*.js",
		"lint:stylelint.report.json": "stylelint --config stylelint.config.js -f json -o reports/stylelintreport.json Assets/Scss/**/*.scss",
		"lint:stylelint.report.junit": "stylelint --config stylelint.config.js --custom-formatter node_modules/stylelint-checkstyle-reporter/dist/stylelint-checkstyle-reporter.mjs -o reports/stylelintreport.junit.xml Assets/Scss/**/*.scss",
		"lint:stylelint.report.verbose": "stylelint --config stylelint.config.js -f verbose -o reports/stylelintreport.verbose.txt Assets/Scss/**/*.scss"
	},
	"author": "ITZBund",
	"license": "MIT",
	"devDependencies": {
		"autoprefixer": "^10.4.17",
		"copy-webpack-plugin": "^12.0.2",
		"css-loader": "^6.10.0",
		"eslint": "^8.56.0",
		"eslint-config-standard": "^17.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-webpack-plugin": "^4.0.1",
		"fast-glob": "^3.3.2",
		"mini-css-extract-plugin": "^2.8.0",
		"path": "^0.12.7",
		"postcss": "^8.4.35",
		"postcss-inline-svg": "^6.0.0",
		"postcss-loader": "^8.1.0",
		"postcss-preset-env": "^9.3.0",
		"postcss-pxtorem": "^6.1.0",
		"sass": "^1.70.0",
		"sass-loader": "^14.1.0",
		"style-loader": "^3.3.4",
		"stylelint": "^16.2.1",
		"stylelint-checkstyle-reporter": "^1.0.0",
		"stylelint-config-recommended": "^14.0.0",
		"stylelint-config-recommended-scss": "^14.0.0",
		"stylelint-config-standard": "^36.0.0",
		"stylelint-order": "^6.0.4",
		"stylelint-scss": "^6.1.0",
		"stylelint-webpack-plugin": "^5.0.0",
		"webpack": "^5.90.1",
		"webpack-cli": "^5.1.4",
		"xml2js": "^0.6.2"
	}
}
